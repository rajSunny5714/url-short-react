{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://url-shorterner-sb.onrender.com/api/:path*"
    },
    {
      "source": "/(login|register|dashboard|about)",
      "destination": "/index.html"
    },
    {
      "source": "/:shortCode([a-zA-Z0-9]+)",
      "destination": "https://url-shorterner-sb.onrender.com/:shortCode"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
